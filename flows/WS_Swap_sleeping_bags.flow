    <screens>
        <name>input_Swap_bags</name>
        <label>Swap bags</label>
        <locationX>387</locationX>
        <locationY>248</locationY>
        <allowBack>false</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <connector>
            <targetReference>dec_UsedBag</targetReference>
        </connector>
        <fields>
            <name>txt_Swap</name>
            <fieldText>&lt;p&gt;&lt;span style=&quot;color: rgb(0, 0, 0);&quot;&gt;You&apos;re about to swap sleeping bag, &lt;/span&gt;&lt;span style=&quot;color: rgb(0, 0, 0); background-color: rgb(255, 255, 255);&quot;&gt;{!Get_Sleeping_Bag.Name}&lt;/span&gt;&lt;span style=&quot;color: rgb(0, 0, 0);&quot;&gt;. &lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>input_Whats_wrong_with_bag</name>
            <choiceReferences>cho_Cleaning</choiceReferences>
            <choiceReferences>cho_Damaged</choiceReferences>
            <choiceReferences>cho_Missing</choiceReferences>
            <dataType>String</dataType>
            <fieldText>What&apos;s wrong with bag?</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>input_Select_a_New_Sleeping_Bag</name>
            <choiceReferences>rch_AvailableSleepingBags</choiceReferences>
            <choiceReferences>ch_UsedBag</choiceReferences>
            <dataType>String</dataType>
            <fieldText>Select a New Sleeping Bag</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>Input_Used_Bags</name>
            <choiceReferences>rcs_UsedBags</choiceReferences>
            <choiceReferences>NULL</choiceReferences>
            <dataType>String</dataType>
            <fieldText>Used Bags</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>input_Select_a_New_Sleeping_Bag</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <elementReference>ch_UsedBag</elementReference>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>txt_usedbags</name>
            <fieldText>&lt;p&gt;Used bags listed are: those with less than 3 uses and lasted used more than 5 days ago.&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>input_Select_a_New_Sleeping_Bag</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <elementReference>ch_UsedBag</elementReference>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>Swap_Comments</name>
            <dataType>String</dataType>
            <fieldText>Swap Comments</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>false</showHeader>
    </screens>
    <screens>
