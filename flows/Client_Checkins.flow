    <decisions>
        <description>Used to determine if any previous checkins were found in the last 4 months</description>
        <name>Previous_Checkins_Found</name>
        <label>Previous Checkins Found</label>
        <locationX>380</locationX>
        <locationY>85</locationY>
        <defaultConnector>
            <targetReference>asg_set_date_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No checkins found</defaultConnectorLabel>
        <rules>
            <name>out_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Checkins</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
