            </value>
        </filters>
        <object>Checkin__c</object>
        <outputReference>sob_tmp_Checkin</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Previous_Night__c</queriedFields>
        <queriedFields>Checkin_Date_Time__c</queriedFields>
        <sortField>Checkin_Date_Time__c</sortField>
