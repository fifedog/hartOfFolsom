    <recordLookups>
        <description>Used to find the client&apos;s previous check-ins if any.</description>
        <name>Get_Checkins</name>
        <label>Get Checkins</label>
        <locationX>260</locationX>
        <locationY>83</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Previous_Checkins_Found</targetReference>
        </connector>
        <filters>
            <field>Checkin_Date_Time__c</field>
