    <decisions>
        <description>If the Contact lookup was not found</description>
        <name>Contact_Found</name>
        <label>Contact Found</label>
        <locationX>182</locationX>
        <locationY>285</locationY>
        <defaultConnector>
            <targetReference>scr_Check_In_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Found</defaultConnectorLabel>
        <rules>
            <name>out_Client_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>lkp_Contact.recordId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Checkin_for_Found_Contact</targetReference>
            </connector>
            <label>Client Found</label>
        </rules>
    </decisions>
    <decisions>
        <description>was the contact already found</description>
        <name>dec_Duplicate_Contact_Found</name>
        <label>Duplicate Contact Found</label>
        <locationX>663</locationX>
        <locationY>302</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>out_Duplicate_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contact_Duplication_check</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Contact</targetReference>
            </connector>
            <label>Duplicate Found</label>
        </rules>
        <rules>
            <name>out_Not_found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Contact_Duplication_check</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Client</targetReference>
            </connector>
            <label>Not found</label>
        </rules>
    </decisions>
    <decisions>
        <description>See&apos;s if this client already has a checkin</description>
        <name>Did_this_Client_already_checkin_today</name>
        <label>Did this Client already checkin today</label>
        <locationX>453</locationX>
        <locationY>604</locationY>
        <defaultConnector>
            <targetReference>Create_Checkin</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>out_Yes_Already_Checked_in</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>frm_GetCheckin_Date</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>frm_Today</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>scr_AlreadyCheckin</targetReference>
            </connector>
            <label>Yes, Already Checked-in</label>
        </rules>
    </decisions>
    <description>Used as the checkin screen for the winter shelter

Change Log:
PF Dec 4 2019: Added gender to new contact screen
PF Dec 3 2019: Updated initial screen, removed Fname and Lname. Added additional screen when not found.</description>
    <dynamicChoiceSets>
        <name>pck_Gender</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>Gender__c</picklistField>
        <picklistObject>Contact</picklistObject>
    </dynamicChoiceSets>
    <formulas>
        <description>Sets the First name in proper case. Used to be sure data goes into the system properly</description>
        <name>frm_FirstNameProper</name>
        <dataType>String</dataType>
        <expression>UPPER( LEFT({!input_First_Name} , 1) ) &amp; LOWER( MID({!input_First_Name} , 2, LEN({!input_First_Name} ) ) )</expression>
    </formulas>
    <formulas>
        <description>Used to convert the Checkin Date/Time to date only</description>
        <name>frm_GetCheckin_Date</name>
        <dataType>Date</dataType>
        <expression>DateValue({!Get_todays_checkins.Checkin_Date_Time__c})</expression>
    </formulas>
    <formulas>
        <name>frm_LastName_Proper</name>
        <dataType>String</dataType>
        <expression>UPPER( LEFT({!input_Last_Name} , 1) ) &amp; LOWER( MID({!input_Last_Name} , 2, LEN({!input_Last_Name} ) ) )</expression>
    </formulas>
    <formulas>
        <name>frm_Today</name>
        <dataType>Date</dataType>
        <expression>Today()</expression>
    </formulas>
